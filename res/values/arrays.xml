	</string-array>

	<!-- States of task (all tasks) -->
	<string-array name="resultStates">
		<item>Novo</item>                <!-- 0 RESULT_NEW               -->
		<item>Fazendo Download</item>    <!-- 1 RESULT_FILES_DOWNLOADING -->
		<item>Download concluído</item>  <!-- 2 RESULT_FILES_DOWNLOADED  -->
		<item>Erro de computação</item>  <!-- 3 RESULT_COMPUTE_ERROR     -->
		<item>Fazendo Upload</item>      <!-- 4 RESULT_FILES_UPLOADING   -->
		<item>Pronto para relatar</item> <!-- 5 RESULT_FILES_UPLOADED    -->
		<item>Abortado</item>            <!-- 6 RESULT_ABORTED           -->
	</string-array>
	<!-- States of task (active tasks only) -->
	<string-array name="activeTaskStates">
		<item>Pronto para iniciar</item>  <!-- 0 PROCESS_UNINITIALIZED (process doesn't exist yet) -->
		<item>Executando</item>         <!-- 1 PROCESS_EXECUTING (process is running, as far as we know) -->
		<item>Encerrado</item>          <!-- 2 *PROCESS_EXITED -->
		<item>Encerrado</item>          <!-- 3 *PROCESS_WAS_SIGNALED     -->
		<item>Encerrado</item>          <!-- 4 *PROCESS_EXIT_UNKNOWN   -->
		<item>Abortado</item>         <!-- 5 PROCESS_ABORT_PENDING (process exceeded limits; send "abort" message, waiting to exit)   -->
		<item>Abortado</item>         <!-- 6 *PROCESS_ABORTED          -->
		<item>Falha ao iniciar</item> <!-- 7 *PROCESS_COULDNT_START   -->
		<item>Encerrando</item>         <!-- 8 PROCESS_QUIT_PENDING (we've sent it a "quit" message, waiting to exit) -->
		<item>Aguardando execução</item>  <!-- 9 PROCESS_SUSPENDED (we've sent it a "suspend" message)        -->
	</string-array>

